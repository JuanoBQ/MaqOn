{
  "kind": "collectionType",
  "collectionName": "blog_tags",
  "info": {
    "singularName": "blog-tag",
    "pluralName": "blog-tags",
    "displayName": "Blog Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tag": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "uid",
      "targetField": "tag",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "blog_tags"
    }
  }
}
